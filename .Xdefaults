! Generic
xft.dpi: 96
xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight

Xcursor*theme: DMZ-White
Xcursor.size:  24

! Application Specific
urxvt*depth: 32
urxvt*background: [75]#000000
urxvt*foreground:#eeeeee

urxvt*scrollBar:false
urxvt*saveLines:32767
urxvt*color0:#eeeeee
urxvt*color1:#CC0000
urxvt*color2:#6AAD2B
urxvt*color3:#C4A000
urxvt*color4:#3465A4
urxvt*color5:#948696
urxvt*color6:#06989A
urxvt*color7:#EEEEEE
urxvt*color8:#666666
urxvt*color9:#EF2929
urxvt*color10:#8AE234
urxvt*color11:#FCE94F
urxvt*color12:#729FCF
urxvt*color13:#BCACBB
urxvt*color14:#34E2E2
urxvt*color15:#FFFFFF
urxvt*colorBD:#FFFFFF
urxvt*cursorColor:#83C048
urxvt*highlightColor:#3465a4
urxvt*font:xft:Terminus:dpi=96
urxvt*boldFont:xft:Terminus:bold:dpi=96
urxvt*geometry:100x30

! make web links clicky
urxvt.perl-ext-common: default,matcher
urxvt.urllauncher: chromium-browser
urxvt.matcher.button: 2
urxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
