cpu_avg_samples 2
net_avg_samples 2
update_interval .5
default_color white
use_spacer right
no_buffers yes
uppercase no
background no

TEXT
[ $nodename : $kernel : $uptime : ${execi 120 /home/haruko/.scripts/updatecount.sh} Upd ] [ CPU: ${cpu cpu1}% ${cpu cpu2}% ] [ RAM: ${memperc}% ]${execi 300 /home/haruko/.scripts/hddmon.sh} [ NET: ${downspeed eth0} down | ${upspeed eth0} up ] [ Vol: ${exec /home/haruko/.scripts/getVolume.sh}%  ] [ $time ]
